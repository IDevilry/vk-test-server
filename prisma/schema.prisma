generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Place {
  id_place     Int     @id @default(autoincrement())
  place_name   String
  description  String
  photo_url    String
  address      String?
  latitude     Float?
  longtitude   Float?
  city_name    String
  country_name String
}
